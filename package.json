{
  "name": "f5-landings",
  "version": "0.0.1",
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "jquery": "~2.1.4",
    "normalize.css": "~3.0.3"
  },
  "devDependencies": {
    "autoprefixer": "~6.0.3",
    "babel-core": "~5.8.29",
    "babel-eslint": "~4.1.3",
    "babelify": "~6.4.0",
    "browser-sync": "~2.9.11",
    "browserify": "~12.0.0",
    "browserify-shim": "^3.8.12",
    "concurrent-transform": "^1.0.0",
    "del": "~2.0.2",
    "envify": "~3.4.0",
    "foldero": "~0.1.1",
    "glob": "~5.0.15",
    "gulp": "~3.9.0",
    "gulp-awspublish": "^3.0.1",
    "gulp-changed": "~1.3.0",
    "gulp-css-scss": "^0.1.0",
    "gulp-data": "~1.2.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "~1.0.0",
    "gulp-fontello": "^0.4.6",
    "gulp-htmlmin": "~1.2.0",
    "gulp-if": "~2.0.0",
    "gulp-imagemin": "~2.3.0",
    "gulp-jade": "~1.1.0",
    "gulp-load-plugins": "~1.0.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-newer": "~1.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.0.4",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sitemap": "^4.0.0",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.4.2",
    "gulp-util": "~3.0.7",
    "imagemin-pngquant": "~4.2.0",
    "imagemin-svgo": "~4.1.2",
    "jade": "~1.11.0",
    "lodash": "~3.10.1",
    "minimist": "~1.2.0",
    "rimraf": "~2.4.3",
    "robots-generator": "git+ssh://git@github.com/sibsfinx/robots-generator.git#master",
    "submit-sitemap": "^0.1.3",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.5.0 ",
    "wrench": "~1.5.8"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules",
    "build:staging": "gulp build --staging",
    "build:production": "gulp build --production",
    "deploy:staging": "gulp build --staging && gulp s3:staging",
    "deploy:production": "gulp build --production && gulp s3:production"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "bootstrap": {
      "exports": null,
      "depends": "jquery:jQuery"
    }
  },
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "appstoreUrl": "",
    "environment": {
      "development": {
        "apiHost": "http://localhost:3000/api/landing",
        "host": "http://localhost:3000",
        "webApp": "http://localhost:4201",
        "GA_TRACKING_ID": ""
      },
      "staging": {
        "apiHost": "",
        "host": "",
        "webApp": "",
        "GA_TRACKING_ID": ""
      },
      "production": {
        "apiHost": "",
        "host": "",
        "hostname": "",
        "webApp": "",
        "GA_TRACKING_ID": ""
      }
    },
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "fonts": "_fonts",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  }
}
